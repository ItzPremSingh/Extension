{
  "manifest_version": 2,
  "name": "Word Meaning",
  "version": "1.0",
  "description": "Show word meanings on double click.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "https://api.dictionaryapi.dev/*"
  ]
}

